{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "space-before-function-paren": ["error", "never"],
      "no-extra-semi": "off"
    }
}
